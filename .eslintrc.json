{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "node": true
    },
    "rules": {
      "linebreak-style": 0,
      "no-script-url": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/jsx-one-expression-per-line": [0, { "allow": "none" }],
      "padded-blocks": ["error", { "blocks": "never", "classes": "always", "switches": "never" }],
      "indent": [0, 2],
      "comma-dangle": ["error", "never"],
      "react/jsx-indent": ["error", "never"],
      "react/jsx-indent-props": ["error", "never"],
      "space-infix-ops": 0,
      "no-plusplus": 0,
      "implicit-arrow-linebreak": 0,
      "import/prefer-default-export": 0,
      "no-lonely-if": 0,
      "jsx-a11y/click-events-have-key-events": 0,
      "no-undef": 0,
      "class-methods-use-this": 0,
      "no-nested-ternary": 0,
      "no-return-assign": 0,
      "no-else-return": 0,
      "object-curly-newline": 0
    }
  }